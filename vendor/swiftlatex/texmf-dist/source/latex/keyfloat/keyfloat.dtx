% \begin{macro}{\KFLT@frame} \marg{contents}
%
% Frames the contents according to the |f| key.  To be nested for further processing.
%    \begin{macrocode}
\newcommand{\KFLT@frame}[1]
{%
    \ifbool{KFLT@ft}%
        {\KFLTtightframe{#1}}%
        {% not tightframe
            \ifbool{KFLT@f}%
                {\KFLTlooseframe{#1}}%
                {#1}% no frame
        }% not looseframe
}
