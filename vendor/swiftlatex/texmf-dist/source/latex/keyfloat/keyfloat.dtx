    \ifboolexpr{%
        test {\ifstrequal{#2}{H}} or
        bool{KFLT@keywrap}
    }%
        {\end{minipage}\vskip\intextsep}% was [H]
        {% not [H]:
            \ifstrequal{#2}{W}%
            {% [W]
%    \end{macrocode}
% \changes{v2.01}{2019/09/16}{Added \env{keysubfloats} [W].}
%    \begin{macrocode}
                \endminipage%
                \endwrapfloat%
            }%
            {% not [W]:
                \ifstrequal{#2}{M}%
                {% [M]:
%    \end{macrocode}
% \changes{v2.01}{2019/09/16}{Added \env{keysubfloats} [M].}
%    \begin{macrocode}
                    \endKFLT@marginfloat%
                }% [M]
                {% subfloat
                    \IfBooleanTF{#1}% starred?
                        {\end{\KFLT@subgrptype*}}%
                        {\end{\KFLT@subgrptype}}%
                }%
            }% not [W]
        }% not [H]
%    \end{macrocode}
% Unnest the environment:
%    \begin{macrocode}
    \setboolean{KFLT@inkeysubfloats}{false}%
    \KFLT@envignorespaces%
}
