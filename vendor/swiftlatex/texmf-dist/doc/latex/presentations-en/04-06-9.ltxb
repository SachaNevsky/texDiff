% Show page(s) 1,2,3

%% ==== 
\PassOptionsToClass{}{beamer}
\documentclass{exabeamer}


%\StartShownPreambleCommands
\useinnertheme{circles}
\usecolortheme{dove}
%\StopShownPreambleCommands

\begin{document}
\begin{frame}{Highlighted text}
\begin{enumerate}
  \item<3-| alert@3>[0.] This item is shown last, even though it is the first
    one in the list.
  \item<1-| alert@1> The first visible item, but only highlighted on the first
    overlay.
  \item<2| alert@2>  The second item, visible and highlighted only on the second
    overlay.
\end{enumerate}
\end{frame}
\end{document}
