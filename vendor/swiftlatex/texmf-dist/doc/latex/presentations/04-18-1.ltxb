\PassOptionsToClass{}{beamer}
\documentclass{beamer}



\usetheme{PaloAlto}


\begin{document}
\begin{frame}[fragile]{Verbatimtext in einer
  Folie}{Sonderbehandlung von Quellcode}
\setbeamercovered{transparent=25}
\uncover<1->{Die Umgebung \texttt{semiverbatim}:}
\begin{semiverbatim}
\textbackslash{}svnInfo \uncover<2->{%
  $Id: ch3-beamer.tex 206
   2010-05-10 13:10:26Z herbert $}
\end{semiverbatim}

Die normale Umgebung \texttt{verbatim}:
\begin{verbatim}
In einer Umgebung verbatim:
\svnInfo $Id: ch3-beamer.tex 206
     2010-05-10 13:10:26Z herbert $
\end{verbatim}
\end{frame}
\end{document}
